<Window x:Class="FFXIVFashionReport.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="FFXIV Fashion Report Generator" Height="795" Width="1200" WindowStartupLocation="CenterScreen" ResizeMode="NoResize"
        Icon="Background/Icon.ico">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="Auto"/>
            <ColumnDefinition Width="*"/>
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition Height="Auto"/>
            <RowDefinition Height="*"/>
        </Grid.RowDefinitions>

        <Image Source="Background/FashionReportImg.png" Width="1200"/>
        <!--#region Equipment-->
        <!--Weapon-->
        <StackPanel Orientation="Vertical" HorizontalAlignment="Left" Margin="52,126,0,588" Width="294" RenderTransformOrigin="0.5,0.5">
            <TextBox x:Name="Weapon" Width="294" KeyDown="Weapon_KeyDown" Height="64" FontSize="24" />
        </StackPanel>

        <Popup x:Name="Weapon_Popup" PlacementTarget="{Binding ElementName=Weapon}" Placement="Bottom">
            <ListView x:Name="Weapon_Filtered" Width="400" MaxHeight="150" SelectionMode="Single" SelectionChanged="Weapon_Filtered_SelectionChanged">
                <ListView.View>
                    <GridView>
                        <GridViewColumn Header="Icone" Width="50">
                            <GridViewColumn.CellTemplate>
                                <DataTemplate>
                                    <Image Width="32" Height="32">
                                        <Image.Source>
                                            <BitmapImage UriSource="{Binding IconUrl}" />
                                        </Image.Source>
                                    </Image>
                                </DataTemplate>
                            </GridViewColumn.CellTemplate>
                        </GridViewColumn>
                        <GridViewColumn Header="Name" Width="250" DisplayMemberBinding="{Binding Name}"/>
                        <GridViewColumn Header="ID" Width="50" DisplayMemberBinding="{Binding ID}"/>
                    </GridView>
                </ListView.View>
            </ListView>
        </Popup>
        <StackPanel x:Name="Weapon_selectedResultPanel" HorizontalAlignment="Left" VerticalAlignment="Top" Orientation="Horizontal" Margin="0 0 0 10" Visibility="Collapsed">
            <Grid>
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="Auto"/>
                    <ColumnDefinition Width="*"/>
                </Grid.ColumnDefinitions>
                <Image x:Name="Weapon_imgSelectedIcon" Width="{Binding ElementName=Weapon, Path=ActualHeight}" Height="{Binding ElementName=Weapon, Path=ActualHeight}" Stretch="Uniform" VerticalAlignment="Center" Margin="5,0"/>
                <TextBlock x:Name="Weapon_txtSelectedName" Grid.Column="1" VerticalAlignment="Center" Margin="5,0,0,0" FontSize="16"/>
            </Grid>
            <Button x:Name="Weapon_btnRemove" Content="x" Width="20" Height="20" Margin="5 0 0 0" Click="Weapon_btnRemove_Click" />
        </StackPanel>

